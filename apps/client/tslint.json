{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"semicolon": [
			true,
			"never"
		],
		"indent": [
			true,
			"tabs",
			1
		],
		"trailing-comma": [
			true,
			{
				"multiline": "never",
				"singleline": "never"
			}
		],
		"quotemark": [
			true,
			"single"
		],
		"ordered-imports": false
	},
	"rulesDirectory": []
}